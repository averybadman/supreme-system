//Jordan Laurent Cellular Automata Application

import java.util.Scanner;
public class Foothill
{
   //main method
   public static void main(String[] args)
   {
      Automaton aut = new Automaton(30);
      aut.setDisplayWidth(79);
      Scanner inputStream = new Scanner(System.in);
      int ruleNumber;
      String userInput;
      
      //infinite loop that is only broken at user's request
      while(true)
      {
         //attempts to parse string to int until no errors are caught
         try
         {
            System.out.print("\nEnter Rule (0 - 255) or q to quit:  ");
            userInput = inputStream.nextLine();
            if(userInput.toUpperCase().charAt(0) == 'Q')
            {
               inputStream.close();
               return;
            }
            ruleNumber = Integer.parseInt(userInput);
            if(ruleNumber < 0 || ruleNumber > 255)
            {
               System.out.println("Invalid Input.");
               continue;
            }
         }
         catch(NumberFormatException e)
         {
            System.out.println("Invalid Input. Whole Numbers Only Please.");
            continue;
         }
         
         //outputs the specified pattern for 100 lines
         aut.setRule(ruleNumber);
         for(int k = 0; k < 100; k++)
         {
            System.out.println(aut.toStringCurrentGen());
            aut.propagateNewGeneration();
         } 
         //resets the Automaton instance members for subsequent loops
         aut.resetFirstGen();
      }   
   }
}

//Automaton class
class Automaton
{
   // class constants
   public final static int MAX_DISPLAY_WIDTH = 111;
   public final static int MAX_RULE_ARGUMENT = 255;
   public final static int MIN_RULE_ARGUMENT = 0;
   
   // private members
   private boolean rules[];  // allocate rules[8] in constructor
   private String thisGen;   // same here
   private String extremeBit; // bit, "*" or " ", implied everywhere "outside"
   private int displayWidth;  // an odd number so it can be perfectly centered
   
   // public constructor
   public Automaton(int new_rule)
   {
      //checks for valid input
      rules = new boolean[8];   
      if(new_rule < MIN_RULE_ARGUMENT || new_rule > MAX_RULE_ARGUMENT)
         setRule(0);  
      else
         setRule(new_rule);
 
      thisGen = "*";
      extremeBit = " ";
      displayWidth = MAX_DISPLAY_WIDTH;
   }
   //resets fields to first generation status
   public void resetFirstGen()
   {
      thisGen = "*";
      extremeBit = " ";
      return;
   }
   public boolean setRule(int new_rule)
   {    
      //checks for valid input before proceeding
      if(new_rule < MIN_RULE_ARGUMENT || new_rule > MAX_RULE_ARGUMENT)
         return false;
      String tempString = Integer.toString(new_rule, 2);
      int count = 0;
      
      //if binary number is less than 8 digits, sets the respective 
      //unlisted zeroes' respective booleans to false in rules[] array
      for(int k = 0; k < rules.length - tempString.length(); k++)
      {
         rules[k] = false;
         count++;
      }
      //sets any remaining, listed binary numbers to respective booleans
      for(int k = 0; k < tempString.length(); k++)
      {
         if(tempString.charAt(k) == '0')
            rules[k + count] = false;         
         else
            rules[k + count] = true;
      }
      return true;
   }
   //simlpe displayWidth mutator, not much to explain here
   public boolean setDisplayWidth(int width)
   {
      //oh and a modulo to ensure a valid odd number is received. That's worth noting.
      if(width > MAX_DISPLAY_WIDTH || width < 1 || width % 2 == 0)
         return false;
      displayWidth = width;
      return true;
   }
   //toString method for the current generation, to be executed in a loop
   public String toStringCurrentGen()
   {
      StringBuffer helperString = new StringBuffer("");
      int indexRef = Math.abs((thisGen.length() - displayWidth) / 2);
      int keyCount = 0;
      
      //if display width is greater than thisGen.length() we must center the display.
      if(displayWidth > thisGen.length())
      {
         for(int k = 0; k < displayWidth; k++)
         {
            //indexRef helps find the point at which to concatenate the middle portion.
            if(k < indexRef || k >= displayWidth - indexRef)
               helperString.append(extremeBit);
            else if(keyCount < thisGen.length())
               helperString.append(thisGen.charAt(keyCount++));   
            else
               continue;
         }
      }
      else if(displayWidth == thisGen.length())
      {
         helperString.append(thisGen);
      }
      else
      { 
         //using indexRef, we find the middle of an oversized generation to concatenate.
         for(int k = indexRef; k < thisGen.length() - indexRef; k++)
            helperString.append(thisGen.charAt(k));
      } 
      return helperString.toString();    
   }
   public void propagateNewGeneration()
   {
      //referenceString helps return the proper array index to be searched.
      String referenceString = "_***_** _* *_*  _ **_ * _  *_   ";
      StringBuffer helperString = new StringBuffer("");
      String nextGen;
      int indexNumber;
      
      //for loop appends two extreme bits to either end of a StringBuffer helperString.
      for(int k = 0; k < 5; k++)
      {
         if(k == 2)
            helperString.append(thisGen);
         else
            helperString.append(extremeBit);
      }
      nextGen = helperString.toString();
      //indexNumber stores the proper index and sets the proper character accordingly
      for(int k = 0; k < nextGen.length() - 2; k++)
      {       
         indexNumber = referenceString.indexOf(nextGen.substring(k, k+3)) / 4;
         if(rules[indexNumber] == true)
         {
            helperString.setCharAt(k+1, '*');
         }
         else     
            helperString.setCharAt(k+1, ' ');     
      }
      nextGen = helperString.toString();
      
      //extremeBit must be corrected before returning
      if(extremeBit == " " && rules[7] == true)
         extremeBit = "*";
      else if(extremeBit == "*" && rules[0] == false)
         extremeBit = " ";
      
      thisGen = nextGen.substring(1, nextGen.length()-1);
   }
}


